<p id="notice"><%= notice %></p>

<h1>Listing Type Tests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Instruction</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_tests.each do |type_test| %>
      <tr>
        <td><%= type_test.name %></td>
        <td><%= type_test.instruction %></td>
        <td><%= link_to 'Show', type_test %></td>
        <td><%= link_to 'Edit', edit_type_test_path(type_test) %></td>
        <td><%= link_to 'Destroy', type_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type test', new_type_test_path %>
